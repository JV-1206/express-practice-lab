<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students Score</title>
</head>
<body>
    <h1>All Scores</h1>
    
    <ul>
        <% students.forEach(function(t) { %>
            <li>
                <%= t.student %>
                  -
                  <%= t.score %>
                  -
                  <%= t.Boolean ? 'pass' : 'dont pass' %>  


            </li>


        <% } );%>

    </ul>
</body>
</html>